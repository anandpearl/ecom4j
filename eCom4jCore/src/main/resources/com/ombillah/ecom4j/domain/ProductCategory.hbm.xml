    <class name="com.ombillah.ecom4j.domain.ProductCategory" table="PRODUCT_CATEGORY">
    
        <id name="categoryId" column="CATEGORY_ID"></id>
        <property name="categoryName" column="CATEGORY_NAME"></property>
        
    </class>
</hibernate-mapping>
