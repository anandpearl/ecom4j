<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ombillah.ecom4j.domain.ProductSpecificationMaster" table="PRODUCT_SPECIFICATION_MASTER">
    
        <id name="id" column="SPEC_ID"></id>
        <property name="name" column="SPEC_NAME" />   
        <property name="description" column="SPEC_DESC" />
        <property name="category" column="SPEC_CATEGORY" />       

    </class>
</hibernate-mapping>
